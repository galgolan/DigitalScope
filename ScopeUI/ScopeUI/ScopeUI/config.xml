    <GainValues>1,2,4,5,8,10,16,32</GainValues>
    <TriggerLevels></TriggerLevels>
    <TriggerSources>CH1,CH2,EXT</TriggerSources>
    <MaxSampleRate>2000000</MaxSampleRate>
  </HardwareInfo>
  
</Configuration>