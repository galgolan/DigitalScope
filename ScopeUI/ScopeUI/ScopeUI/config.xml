<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  
  <SerialPort>
    <Port>COM3</Port>
    <Baudrate>115200</Baudrate>
    <DataBits>8</DataBits>
    <Parity>NoParity</Parity>
    <StopBits>1</StopBits>
  </SerialPort>
  
  <Interface>
    <WaveformFramesPerSec>25</WaveformFramesPerSec>
